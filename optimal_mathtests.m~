
clear all; close all; clc;
Nsamples = 100000;
for sample_A = 1:15
    space = [1:15, 1:10, 6:15];
    space(space==sample_A)=[];  % make sure we dont sample same number twice in a row
    samples = randsample(space,Nsamples, true);
    %hist(samples,15);
    
    n = sum(samples<sample_A);
    sample_A
    ratio = n/Nsamples
end




